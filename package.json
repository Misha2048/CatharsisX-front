{
  "name": "catharsisx-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@linaria/babel-preset": "^5.0.4",
    "@linaria/core": "^5.0.2",
    "@linaria/react": "^5.0.3",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.54",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "eslint src/**/*.{ts,tsx} && react-app-rewired start",
    "build": "eslint src/**/*.{ts,tsx} && react-app-rewired build",
    "test": "eslint src/**/*.{ts,tsx} && react-app-rewired test",
    "eject": "eslint src/**/*.{ts,tsx} && react-scripts eject",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@linaria/webpack-loader": "^5.0.4",
    "@typescript-eslint/parser": "^5.62.0",
    "customize-cra": "^1.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.3",
    "react-app-rewired": "^2.2.1"
  }
}
